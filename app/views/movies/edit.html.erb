<h3>Update Movie</h3>
<hr>
<div class="container">
  <%= form_for(@movie) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :content %>
    <%= f.text_field :content %>
    <%= f.submit "Update", class: "btn btn-primary" %>
  <% end %>
